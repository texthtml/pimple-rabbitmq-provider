{
    "name": "texthtml/pimple-rabbitmq-provider",
    "description": "Silex service provider for RabbitMQ. Integrates php-amqplib/rabbitmq-bundle into Silex.",
    "keywords": [ "silex", "pimple", "provider", "rabbitmq", "amqp", "message", "queue" ],
    "homepage": "https://github.com/texthtml/pimple-rabbitmq-provider",
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0",
        "pimple/pimple": "^3.0",
        "php-amqplib/rabbitmq-bundle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": { "TH\\RabbitmqProvider\\": "src" }
    },
    "autoload-dev" : {
        "psr-4": { "TH\\RabbitmqProvider\\Tests\\" : "tests/Tests" }
    }
}
