{
    "name": "e-sixt/rabbitmq-service-provider",
    "replace": {
        "fiunchinho/rabbitmq-service-provider": "1.0.1"
    },
    "type": "silex-service-provider",
    "description": "Silex service provider for RabbitMQ. Integrates php-amqplib/rabbitmq-bundle into Silex.",
    "keywords": [ "silex", "provider", "rabbitmq", "amqp", "message", "queue" ],
    "homepage": "https://github.com/e-sixt/rabbitmq-service-provider",
    "license": "MIT",
    "authors": [
        {"name": "Anton StÃ¶ckl"},
        {"name": "Jose Armesto"}
    ],
    "require": {
        "php": ">=5.5.0",
        "pimple/pimple": "~3.0",
        "php-amqplib/rabbitmq-bundle": "~1.9"
    },
    "suggest": {
        "ivoba/console-service-provider": "for the rabbitmq:consumer command"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "fiunchinho\\": "src/fiunchinho" }
    },
    "autoload-dev" : {
        "psr-4": { "fiunchinho\\Tests\\" : "tests/fiunchinho/Tests" }
    }
}
